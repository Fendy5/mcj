page{
background-color: #e6e6e6 
}

.border_reword{
  width: 100%;
  height: 40rpx;
}
.containter_out{
  z-index: 1;
display: flex;
justify-content: center;
}
.containter{
  width: 94%;
  display: flex;
  flex-direction: column;
  background-color: #ffffff;
  border-radius: 15rpx;
}
.header{
display: flex;
justify-content: space-around;
padding-top: 10px;
font-size: 16px;
}
.header_left{
display: flex;
}
.header_right{
padding-top: 5px;
}
.content{
display: flex;
flex-direction: column;
text-align: center;
padding: 50rpx 0;
}
.content_up{
color: #8f8f8f;
font-size: 14px;
}
.content_down{
font-size: 30px;
}
.footer{
display: flex;
justify-content: space-between;
border-top: 1px #a18cd1 solid;
margin: 0 10px;
padding: 8px 5px;
}
